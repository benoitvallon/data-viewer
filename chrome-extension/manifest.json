{
  "name": "Get Bookmarks",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "128": "img/icon-128x128.png"
  },
  "browser_action": {
    "default_title": "My Bookmarks",
    "default_icon": "img/icon-38x38.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "background",
    "bookmarks",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
